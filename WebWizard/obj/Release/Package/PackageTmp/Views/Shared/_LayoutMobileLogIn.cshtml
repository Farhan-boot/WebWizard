<body>





    @RenderBody()







    <!-- jQuery -->

    <script src="~/MobileAssets/scripts/jquery-3.4.1.min.js"></script>
    <!-- Bootstrap Core JS -->
    <script src="~/MobileAssets/scripts/popper.min.js"></script>

    <script src="~/MobileAssets/scripts/bootstrap.min.js"></script>
    <!-- Custom Scroll JS -->

    <script src="~/MobileAssets/scripts/plugins/mcustomscroll/jquery.mCustomScrollbar.js"></script>

    <script src="~/MobileAssets/scripts/luxon-1.11.4.js"></script>


    <script src="~/MobileAssets/scripts/app.js"></script>
    <!-- Add the install script here -->

    <script src="~/MobileAssets/scripts/install.js"></script>

    <script src="/Assets/toastr/toastr.min.js"></script>

    <script>
        // Register service worker.
        if ('serviceWorker' in navigator) {
            window.addEventListener('load', function () {
                navigator.serviceWorker.register('service-worker.js').then(function (registration) {
                    // Registration was successful
                    console.log('ServiceWorker registration successful with scope: ', registration.scope);
                }, function (err) {
                    // registration failed :(
                    console.log('ServiceWorker registration failed: ', err);
                });
            });
        }
    </script>

    <!--WebWizard LogIn script start-->
    <script>
        var logIn = function () {
            var WebWizardLogInModel = {
                Id: "",
                Email: "",
                Password: ""
            };
            WebWizardLogInModel.Email = $("#email").val();
            WebWizardLogInModel.Password = $("#password").val();

            if (WebWizardLogInModel.Email != "" && WebWizardLogInModel.Password != "") {
                $.ajax({
                    type: "post",
                    url: "/Mobile/WizerdLogIn",
                    data: WebWizardLogInModel,
                    success: function (data) {
                        if (data.Status == false) {
                            toastr.error("Please type right email or password! try again", "Error");
                        }
                        else {
                            // Set a cookie email and password
                            //$.cookie('email', data.Email);
                            //$.cookie('password', data.Password);
                            window.location.pathname = 'Mobile/WizerdDashboard';
                        }
                    }
                })
            }
            else {
                toastr.error("Please type right email or password! try again", "Error");
            }
        }
    </script>
    <!--WebWizard LogIn script End-->


</body>


</html>


