    <!--WebWizard LogIn script start-->
    <script>
        var logIn = function () {
            var WebWizardLogInModel = {
                Id: "",
                Email: "",
                Password: ""
            };
            WebWizardLogInModel.Email = $("#email").val();
            WebWizardLogInModel.Password = $("#password").val();

            if (WebWizardLogInModel.Email != "" && WebWizardLogInModel.Password != "") {
                $.ajax({
                    type: "post",
                    url: "/LogIn/Index",
                    data: WebWizardLogInModel,
                    success: function (data) {
                        if (data.Status == false) {
                            toastr.error("Please type right email or password! try again", "Error");
                        }
                        else {
                            // Set a cookie email and password
                            $.cookie('email', data.Email);
                            $.cookie('password', data.Password);
                            window.location.pathname = 'WebWizardDashboard/Profile';
                        }
                    }
                })
            }
            else {
                toastr.error("Please type right email or password! try again", "Error");
            }
        }
    </script>
    <!--WebWizard LogIn script End-->
